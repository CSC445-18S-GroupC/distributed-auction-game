<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="csc445.groupc.distauction.View.LoginView">
  <grid id="27dc6" binding="loginPanel" layout-manager="GridBagLayout">
    <constraints>
      <xy x="20" y="20" width="800" height="600"/>
    </constraints>
    <properties>
      <minimumSize width="800" height="600"/>
      <preferredSize width="400" height="300"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="5f1ae" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <opaque value="true"/>
          <text value="Please Enter your username:"/>
          <verifyInputWhenFocusTarget value="true"/>
        </properties>
      </component>
      <hspacer id="fc0dc">
        <constraints>
          <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
      </hspacer>
      <vspacer id="e9656">
        <constraints>
          <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
      </vspacer>
      <component id="c28df" class="javax.swing.JTextField" binding="usernameField">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="-1"/>
          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value=""/>
        </properties>
      </component>
      <component id="504b7" class="javax.swing.JButton" binding="hostButton">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <minimumSize width="99" height="32"/>
          <preferredSize width="99" height="60"/>
          <selected value="false"/>
          <text value="Host Game"/>
        </properties>
      </component>
      <component id="4d2c1" class="javax.swing.JButton" binding="joinButton">
        <constraints>
          <grid row="3" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <preferredSize width="95" height="60"/>
          <text value="Join Game"/>
        </properties>
      </component>
      <component id="e210" class="javax.swing.JButton" binding="rejoinButton">
        <constraints>
          <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Rejoin Game"/>
          <toolTipText value="Rejoins the previously played game"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
